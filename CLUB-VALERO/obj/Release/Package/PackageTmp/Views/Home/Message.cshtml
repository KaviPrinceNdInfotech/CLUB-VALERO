
@{
    ViewBag.Title = "Send message";
    Layout = "~/Views/Shared/_AccountLayout.cshtml";
}
<!DOCTYPE html>
<html lang="en">
<head>
   
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/Lib/ContactUsPageCSS/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Lib/ContactUsPageCSS/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Lib/ContactUsPageCSS/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Lib/ContactUsPageCSS/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Lib/ContactUsPageCSS/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Lib/ContactUsPageCSS/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Lib/ContactUsPageCSS/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Lib/ContactUsPageCSS/css/main.css">
    <!--===============================================================================================-->
</head>
<body>

    <div class="contact1">
        <div class="container-contact1">
            <div class="contact1-pic js-tilt" data-tilt>
                <img src="~/Lib/ContactUsPageCSS/images/Image1.jpg" alt="IMG">
            </div>

            <form class="contact1-form validate-form" action="/Home/Message" method="post">
                <span class="contact1-form-title">
                    Get in touch
                </span>

                <div class="wrap-input1 validate-input" data-validate="Name is required">
                    <input class="input1" type="text" name="Name" placeholder="Name" required>
                    <span class="shadow-input1"></span>
                </div>
                <div class="wrap-input1 validate-input" data-validate="Mobile is required">
                    <input class="input1" type="text" name="MobileNo" placeholder="Mobile No" required>
                    <span class="shadow-input1"></span>
                </div>
                <div class="wrap-input1 validate-input" data-validate="Profission is required">
                    <input class="input1" type="text" name="Profission" placeholder="Profission">
                    <span class="shadow-input1"></span>
                </div>

                <div class="wrap-input1 validate-input" data-validate="Address is required">
                    <textarea class="input1" name="Address" placeholder="Address"></textarea>
                    <span class="shadow-input1"></span>
                </div>

                <div class="container-contact1-form-btn">
                    <button class="contact1-form-btn">
                        <span>
                            Send Email
                            <i class="fa fa-long-arrow-right" aria-hidden="true"></i>
                        </span>
                    </button>
                </div>
            </form>
        </div>
    </div>




    <!--===============================================================================================-->
    <script src="~/Lib/ContactUsPageCSS/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Lib/ContactUsPageCSS/vendor/bootstrap/js/popper.js"></script>
    <script src="~/Lib/ContactUsPageCSS/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Lib/ContactUsPageCSS/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Lib/ContactUsPageCSS/vendor/tilt/tilt.jquery.min.js"></script>
    <script>
		$('.js-tilt').tilt({
			scale: 1.1
		})
    </script>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-23581568-13"></script>
    <script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'UA-23581568-13');
    </script>

    <!--===============================================================================================-->
    <script src="~/Lib/ContactUsPageCSS/js/main.js"></script>

</body>
</html>


