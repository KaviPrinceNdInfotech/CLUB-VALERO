@model CLUB_VALERO_DB.Entities.Member

@{
    ViewBag.Title = "Update Member";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
</head>
<script>
    $(document).ready(function () {
        $(".c1").hide();
        $(".c2").hide();
        $(".c3").hide();

        $("select").click(function () {
          var data=  $("#selectvalue").val();
            if (1 == data) {
                $(".c1").toggle();
            }
            if (2 == data) {
                $(".c1").toggle();
                $(".c2").toggle();
            }
            if (3 == data) {
                $(".c1").toggle();
                $(".c2").toggle();
                $(".c3").toggle();
            }

        })
        $('.DD_Cheque').hide();
        $(".Cash").hide();
        $('.debit').hide();
        $("select").click(function () {
            var data1 = $("#ModeOfPayment").val();
            if ("Demand Draft" == data1 || "Cheque" == data1) {
                $('.DD_Cheque').show();
                $(".Cash").hide();
                $('.debit').hide();
            }
            if ("Cash" == data1) {
                $('.Cash').show();
                $('.DD_Cheque').hide();
                $('.debit').hide();
            }
            if ("Credit Card" == data1 || "Debit Card" == data1) {
                $(".debit").show();
                $('.DD_Cheque').hide();
                $('.Cash').hide();
            }

            if ("Demand Draft" == data1) {
                $('.DD_Cheque').show();
                $(".Cash").hide();
                $('.debit').hide();
            }
        })

        $("select").click(function () {
            var data2 = $("#ApptType").val();
            if ("STUDIO" == data2) {
                $("#NoOf_Night_Year").val("6N/7Days");
                $("#Occupants").val("2 Child 2 Adult");
            }
            if ("1 BEDROOM" == data2) {
                $("#NoOf_Night_Year").val("6N/7Days");
                $("#Occupants").val("2 Child 3 Adult");
            }
        })


        $("select").click(function () {
            var Tdate = $("#Tenure").val();

            var d = new Date();
            var strDate1 = (d.getDate()) + "-" + (d.getMonth() + 1) + "-" + (d.getFullYear() + parseInt(Tdate));
            $("#TenureDate").val(strDate1);

        })

    })
</script>


