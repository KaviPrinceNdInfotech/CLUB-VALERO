@model IEnumerable<CLUB_VALERO_DB.Entities.Member>

@{
    ViewBag.Title = "BookMember";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2 class="text-warning mt-4">BookMember</h2>

<table class="table table-hover mt-2">
    <thead>
        <tr class="bg-info text-white">
            <th>MemberShip Id</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Mobile</th>
            <th>Email</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
            {
        <tr>
            <td> <b class="text-warning h5 font-weight-bold text-warning"><a href="/Admin/BookingList?id=@item.Id">@item.MemberShipId</a> </b>   </td>
            <td>@item.M_FirstName</td>
            <td>@item.M_LastName</td>
            <td>@item.Mobile</td>
            <td>@item.Email</td>

        </tr>
            }
    </tbody>
</table>

