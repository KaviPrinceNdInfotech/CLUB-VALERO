@model IEnumerable<CLUB_VALERO_DB.Entities.Member>

@{
    ViewBag.Title = "EmiAllUser";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>
$(document).ready(function(){
  $("#myInput").on("keyup", function() {
    var value = $(this).val().toLowerCase();
    $("#myTable tr").filter(function() {
      $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
    });
  });
});
</script>
<input id="myInput" type="text" class="form-control col-md-6" placeholder="Search.." style="margin-top: 21px;">
<table class="table  table-hover table-bordered" style="margin-top:30px;">
    <tr class="bg-info text-white">

        <th>
            <label>User Id</label>
        </th>
        <th>
            <label>Name</label>
        </th>
        <th>
            <label>Mobile No</label>
        </th>
        <th>
            <label>Gender</label>
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tbody id="myTable">

            <tr>

                <td>
                    <a class="text-primary" href="/Admin/AmPaymentList?id=@item.Id">@item.MemberShipId</a>
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.M_FirstName)
                </td>
                <td>@item.Mobile</td>
                <td>@item.Co_Gender</td>

            </tr>

        </tbody>
    }

</table>
